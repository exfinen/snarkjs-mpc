rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /{ceremony}/ptau {
      allow read: if true; 
    }
    match /{ceremony}/circuits/{circuit}/r1cs {
      allow read: if true;
    }
    match /{ceremony}/circuits/{circuit}/zkey {
      allow read, write: if request.auth != null;
    }
  }
}
